@article{babson_generalized_2000,
	title = {Generalized {Permutation} {Patterns} and a {Classification} of the {Mahonian} {Statistics}},
	volume = {44},
	issn = {1286-4889},
	url = {https://www.emis.de/journals/SLC/wpapers/s44stein.html},
	abstract = {We introduce generalized permutation patterns, where we allow the requirement that two adjacent letters in a pattern must be adjacent in the permutation. We show that essentially all Mahonian permutation statistics in the literature can be written as linear combinations of such patterns. Almost all known Mahonian permutation statistics can be written as linear combinations of patterns of length at most 3. There are only fourteen possible such Mahonian statistics, which we list. Of these, eight are known and we give proofs for another three. The remaining three we conjecture to be Mahonian. We also give an explicit numerical description of the combinations of patterns a Mahonian statistic must have, depending on the maximal length of its patterns.},
	number = {B44b},
	journal = {Séminaire Lotharingien de Combinatoire},
	author = {Babson, Eric and Steingrímsson, Einar},
	year = {2000},
	pages = {18 pp.}
}

@article{bousquet-melou_2+2-free_2010,
	title = {(2+2)-free posets, ascent sequences and pattern avoiding permutations},
	volume = {117},
	issn = {0097-3165},
	url = {http://www.sciencedirect.com/science/article/pii/S0097316509001885},
	doi = {10.1016/j.jcta.2009.12.007},
	abstract = {We present bijections between four classes of combinatorial objects. Two of them, the class of unlabeled (2+2)-free posets and a certain class of involutions (or chord diagrams), already appeared in the literature, but were apparently not known to be equinumerous. We present a direct bijection between them. The third class is a family of permutations defined in terms of a new type of pattern. An attractive property of these patterns is that, like classical patterns, they are closed under the action of the symmetry group of the square. The fourth class is formed by certain integer sequences, called ascent sequences, which have a simple recursive structure and are shown to encode (2+2)-free posets and permutations. Our bijections preserve numerous statistics. We determine the generating function of these classes of objects, thus recovering a non-D-finite series obtained by Zagier for the class of chord diagrams. Finally, we characterize the ascent sequences that correspond to permutations avoiding the barred pattern 31¯524¯ and use this to enumerate those permutations, thereby settling a conjecture of Pudwell.},
	language = {en},
	number = {7},
	journal = {Journal of Combinatorial Theory, Series A},
	author = {Bousquet-Mélou, Mireille and Claesson, Anders and Dukes, Mark and Kitaev, Sergey},
	month = oct,
	year = {2010},
	keywords = {-free poset, Ascent sequence, Chord diagram, Encode, Enumerate, Involution, Non-D-finite series, Pattern avoiding permutation},
	pages = {884--909}
}