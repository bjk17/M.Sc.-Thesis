@article{tannock_equivalence_2018,
	title = {Equivalence classes of mesh patterns with a dominating pattern},
	copyright = {Attribution-NoDerivs 2.0 Generic},
	issn = {1365-8050},
	url = {https://dmtcs.episciences.org/4265},
	doi = {10.23638/dmtcs-19-2-6},
	abstract = {Two mesh patterns are coincident if they are avoided by the same set of permutations, and are Wilf-equivalent if they have the same number of avoiders of each length. We provide sufficient conditions for coincidence of mesh patterns, when only permutations also avoiding a longer classical pattern are considered. Using these conditions we completely classify coincidences between families containing a mesh pattern of length 2 and a classical pattern of length 3. Furthermore, we completely Wilf-classify mesh patterns of length 2 inside the class of 231-avoiding permutations.},
	journal = {Discrete Mathematics \& Theoretical Computer Science ; Vol. 19 no. 2},
	author = {Tannock, Murray and Ulfarsson, Henning},
	year = {2018},
	keywords = {05A05, 05A15, Mathematics - Combinatorics},
	pages = {Permutation Patterns 2016 ; 13658050}
}

